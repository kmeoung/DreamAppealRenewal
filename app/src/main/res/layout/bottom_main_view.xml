<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/ll_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <!--하단 Nav-->
    <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/bottom_nav_underline"
            android:contentDescription="@string/str_content_description" />

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="44dp">

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_timeline"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_bottom_tab"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:src="@drawable/ic_home"
                android:tint="@color/white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent="0.2" />

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_home"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_bottom_tab"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:src="@drawable/ic_dashboard"
                android:tint="@color/white"
                app:layout_constraintStart_toEndOf="@+id/iv_timeline"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent="0.2" />

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_add_board"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_bottom_tab"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:src="@drawable/ic_add_circle"
                android:tint="@color/white"
                app:layout_constraintStart_toEndOf="@+id/iv_home"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent="0.2" />

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_notification"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_bottom_tab"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:src="@drawable/ic_notifications"
                android:tint="@color/white"
                app:layout_constraintStart_toEndOf="@+id/iv_add_board"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent="0.2" />

        <TextView
                android:id="@+id/tv_notification"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/ic_circle_orange"
                android:fontFamily="@font/bmjua"
                android:gravity="center"
                android:text="9"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="@id/iv_notification"
                app:layout_constraintStart_toStartOf="@id/iv_notification"
                app:layout_constraintTop_toTopOf="@id/iv_notification" />

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_profile"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_bottom_tab"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:src="@drawable/ic_person"
                android:tint="@color/white"
                app:layout_constraintStart_toEndOf="@id/iv_notification"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent="0.2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

